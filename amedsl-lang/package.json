{
  "name": "amedsl-lang",
  "publisher": "nova-energy-consulting",
  "author": {
    "name": "Carlos Gamez",
    "email": "info@novaenergy.consulting",
    "url": "https://www.novaenergy.consulting"
  },
  "icon": "fileicons/amedsl-logo-200.png",
  "galleryBanner": {
    "color": "#f2f2f2",
    "theme": "dark"
  },
  "displayName": "Asset Management Engine DSL",
  "description": "A domain-specific language for asset management modelling tasks",
  "repository": {
    "url": "https://amedsl-lang.novaenergy.digital"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "amedsl",
        "aliases": [
          "Asset Modelling Engine DSL",
          "amedsl"
        ],
        "extensions": [
          ".ame"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./fileicons/amedsl-logo.png",
          "dark": "./fileicons/amedsl-logo.png"
        }
      }
    ],
    "snippets": [
      {
        "language": "amedsl",
        "path": "./snippets/amedsl.snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "amedsl",
        "scopeName": "source.ame",
        "path": "./syntaxes/amedsl.tmLanguage.json"
      }
    ]
  }
}
